{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      },
      "Https": {
        "Url": "https://+:443"
      }
    }
  },
  "Certificate": {
    "DefaultKeyPath": "../../script/tls.key",
    "DefaultPath": "../../script/tls.crt"
  },
  "Receiver": {
    "ControllerUrl": "http://localhost:8000"
  },
  "ServiceDiscovery": {
    "Consul": {
      "Address": "http://127.0.0.1:8500",
      "RefreshIntervalSeconds": 60
    }
  }
}
